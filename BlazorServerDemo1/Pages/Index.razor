@page "/"

@* @inject IDemo _demo
@inject IDemo _demo2 *@

@inject IEnumerable<IDemo> _demos
@inject ProcessDemo _processDemo


<PageTitle>Index</PageTitle>

<h1>Hello, world!</h1>

@foreach (var item in _demos)
{
    @if (item is ILocalDemo)
    {
        <h2>Demo: @item.StartUpTime.ToString("hh:mm:ss ffffff")</h2>

    }

    @if (item is IUtcDemo)
    {
        <h2>UTC Demo: @item.StartUpTime.ToString("hh:mm:ss ffffff")</h2>
    }
}
@* <h2>Demo Time: @_demo.StartUpTime.ToString("hh:mm:ss ffffff")</h2>
<h2>Demo2  Time: @_demo.StartUpTime.ToString("hh:mm:ss ffffff")</h2>
 *@Welcome to your new app.
<h3>Days in Month: @_processDemo.GetDaysInMonth()</h3>

<SurveyPrompt Title="How is Blazor working for you?" />
